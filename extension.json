			"scripts": [
				"ui/preferences/ChannelPreferences.js"
			],
			"dependencies": [
				"oojs-ui"
			]
		},
		"ext.notifyme.subscription.set.bootstrap": {
			"scripts": [
				"ui/subscriptionSet/editor/SubscriptionSetEditor.js",
				"ui/subscriptionSet/SubscriptionSet.js"
			],
			"messages": [
				"notifyme-ui-edit",
